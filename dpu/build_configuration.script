// Every declared tasklet may contribute to the process (see debug.h).
// The output mailbox can store statistical reports, as defined by dpu_tasklet_stats_t
tasklet add 0..15 2048 main 0
// Mutexes:
//  - 0: request pool
//  - 1: result pool
//  - 2: miscellaneous
mutex 3
barrier add 0 16
save
quit